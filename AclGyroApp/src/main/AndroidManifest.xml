<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aclgyro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AclGyro">
        <activity android:name=".activity.OpeningActivity" android:theme="@style/Theme.AclGyro.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.FormActivity" />
        <activity android:name=".activity.GyroActivity" />
        <activity android:name=".activity.AclActivity" />
        <activity android:name=".activity.MainActivity"/>
    </application>

</manifest>